{
  "name": "portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install-deps": "rm -rf node_modules && rm yarn.lock && cd client && rm -rf node_modules && rm yarn.lock && cd ../server rm -rf node_modules && rm yarn.lock && cd ../ && yarn install && cd client && yarn install && cd ../server && yarn install && cd ../",
    "start": "concurrently \"cd server && yarn start\" \"cd client && yarn start\"",
    "lint": "eslint .",
    "test:client": "cd client && npm test",
    "test:client:coverage": "cd client && npm run coverage",
    "test:server": "cd server && npm test",
    "test:server:watch": "cd server && npm run test:watch",
    "test:server:coverage": "cd server && npm run coverage"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.2.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "@babel/register": "^7.0.0",
    "concurrently": "4.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
