{
  "name": "portfolio2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.5.2",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "express": "4.13.3",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "parallax-js": "git+https://github.com/wagerfield/parallax.git",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-parallax": "^1.4.4",
    "react-redux": "^5.0.6",
    "react-reveal": "^0.1.1",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.1",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "spotify-web-api-js": "^0.22.0",
    "spotify-web-api-node": "^2.4.0"
  },
  "devDependencies": {
    "concurrently": "3.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/assets/styles/sass -o ./src/assets/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/assets/styles/sass -o ./src/assets/styles/css --watch --recursive",
    "start-js": "react-scripts start",
    "server": "node ./src/server/server.js",
    "start": "concurrently \"npm-run-all -p watch-css start-js\" \"npm run server\"",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
