{
  "name": "portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install-deps": "rm -rf node_modules && cd client && rm -rf node_modules && cd ../server rm -rf node_modules && cd ../ && yarn && cd client && yarn && cd ../server && yarn && cd ../",
    "start": "concurrently \"cd server && yarn start\" \"cd client && yarn start\"",
    "lint": "eslint .",
    "test:client": "cd client && npm test",
    "test:client:coverage": "cd client && npm run coverage",
    "test:server": "cd server && npm test",
    "test:server:watch": "cd server && npm run test:watch",
    "test:server:coverage": "cd server && npm run coverage"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0"
  }
}
