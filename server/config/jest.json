{
  "rootDir": "../",
  "coverageDirectory": "<rootDir>/coverage",
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "testRegex": ".*?\\.(test|spec)\\.js$",
  "collectCoverage": false,
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/node_modules/**"
  ],
  "testEnvironment": "node",
  "verbose": true
}

